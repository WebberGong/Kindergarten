{
  "env": {
    "browser": true
  },
  "globals": {
    "angular": true,
    "_": true,
    "$": true,
    "fastclick": true
  },
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "eqeqeq": 2,
    "strict": [
      2,
      "function"
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "no-underscore-dangle": 0,
    "no-redeclare": 0,
    "no-shadow": 0,
    "consistent-return": 0
  }
}
